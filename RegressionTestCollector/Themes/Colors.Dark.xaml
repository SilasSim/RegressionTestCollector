<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <SolidColorBrush x:Key="Brush.SecondaryBackground" Color="Gray"/>
  <SolidColorBrush x:Key="Brush.BackgroundColor" Color="#FF313131"/>
  <SolidColorBrush x:Key="Brush.ForegroundColor" Color="White"/>
  <SolidColorBrush x:Key="Brush.Border" Color="White"/>
  <SolidColorBrush x:Key="Brush.TextBackground" Color="#636363"/>
  <SolidColorBrush x:Key="Brush.Alert" Color="Red"/>
  <SolidColorBrush x:Key="Brush.Success" Color="LimeGreen"/>
  <SolidColorBrush x:Key="Brush.TableHeaderBackground" Color="#252525"/>
  <SolidColorBrush x:Key="Brush.InputBackground" Color="#3A3A3A"/>
  <SolidColorBrush x:Key="Brush.InputBorder" Color="#555555"/>
  <SolidColorBrush x:Key="Brush.ForegroundMuted" Color="#FF9A9A9A"/>
  <SolidColorBrush x:Key="Brush.CellBackground" Color="#3D3D3D"/>
  <SolidColorBrush x:Key="Brush.Divider" Color="#EEEEEE"/>
  <SolidColorBrush x:Key="Brush.MouseOver" Color="#575757"/>
  <SolidColorBrush x:Key="Brush.IsPressed" Color="#424242"/>
  <SolidColorBrush x:Key="Brush.Highlight" Color="#7B7B7B"/>

  <SolidColorBrush x:Key="Brush.Icon.Filter" Color="LightGray"/>
  <SolidColorBrush x:Key="Brush.Icon.FilterHover" Color="Gray"/>
  <SolidColorBrush x:Key="Brush.Icon.FilterActive" Color="Bisque"/>

  <SolidColorBrush x:Key="Brush.Titlebar" Color="Black"/>
  <SolidColorBrush x:Key="Brush.HighlightSearch" Color="Yellow"/>
  <SolidColorBrush x:Key="Brush.HighlightedText" Color="Black"/>

  <SolidColorBrush x:Key="Color.ScrollbarTrack" Color="Transparent"/>
  <SolidColorBrush x:Key="Color.ScrollbarTrackHover" Color="#3D3D3D"/>
  <SolidColorBrush x:Key="Color.ScrollbarThumb" Color="#626262"/>
  <SolidColorBrush x:Key="Color.ScrollbarThumbHover" Color="#737373"/>
  <SolidColorBrush x:Key="Color.ScrollbarThumbActive" Color="#989898"/>
  <SolidColorBrush x:Key="Brush.CircleFill" Color="White"/>
</ResourceDictionary>