<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
  <SolidColorBrush x:Key="Brush.SecondaryBackground" Color="LightGray"></SolidColorBrush>

  <SolidColorBrush x:Key="Brush.BackgroundColor" Color="White"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.ForegroundColor" Color="Black"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Border" Color="Black"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.TextBackground" Color="White"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.TableHeaderBackground" Color="#E2E2E2"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Alert" Color="Red"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Success" Color="LimeGreen"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.InputBackground" Color="White"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.InputBorder" Color="#FFBEBEBE"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.ForegroundMuted" Color="#FF606060"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.CellBackground" Color="White"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Divider" Color="#313131"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.MouseOver" Color="#8B8B8B"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.IsPressed" Color="Silver"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Highlight" Color="#B9B9B9"></SolidColorBrush>


  <SolidColorBrush x:Key="Brush.Icon.Filter" Color="Gray"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Icon.FilterHover" Color="LightGray"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.Icon.FilterActive" Color="Bisque"></SolidColorBrush>

  <SolidColorBrush x:Key="Brush.Titlebar" Color="#EEEEEE"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.HighlightSearch" Color="Yellow"/>
  <SolidColorBrush x:Key="Brush.HighlightedText" Color="Black"/>

  <SolidColorBrush x:Key="Color.ScrollbarTrack" Color="Transparent"></SolidColorBrush>
  <SolidColorBrush x:Key="Color.ScrollbarTrackHover" Color="LightGray"></SolidColorBrush>
  <SolidColorBrush x:Key="Color.ScrollbarThumb" Color="#A3A3A3"></SolidColorBrush>
  <SolidColorBrush x:Key="Color.ScrollbarThumbHover" Color="#888888"></SolidColorBrush>
  <SolidColorBrush x:Key="Color.ScrollbarThumbActive" Color="#626262"></SolidColorBrush>
  <SolidColorBrush x:Key="Brush.CircleFill" Color="Black"/>
</ResourceDictionary>